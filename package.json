{
  "name": "angular-security-course",
  "version": "0.0.0",
  "license": "MIT",
  "notes": {
    "extra-cli-options": "--ssl --ssl-key key.pem --ssl-cert cert.pem"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.json --ssl --ssl-key key.pem --ssl-cert cert.pem",
    "start-server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts --secure",
    "server": "nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "hash": "node ./demos/hash.js",
    "verify-password": "node ./demos/verify-password.js",
    "jwt-hs256": "node demos/JWT/jwt-hs256.js",
    "jwt-check-hs256": "node ./demos/JWT/jwt-check-hs256.js",
    "jwt-rs256": "node ./demos/JWT/jwt-rs256.js",
    "jwt-check-rs256": "node ./demos/JWT/jwt-check-rs256.js",
    "csrf-server": "http-server -c-1 ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.5",
    "@angular/common": "^11.2.5",
    "@angular/compiler": "^11.2.5",
    "@angular/core": "^11.2.5",
    "@angular/forms": "^11.2.5",
    "@angular/platform-browser": "^11.2.5",
    "@angular/platform-browser-dynamic": "^11.2.5",
    "@angular/router": "^11.2.5",
    "@auth0/auth0-angular": "^1.3.2",
    "argon2": "^0.27.1",
    "body-parser": "^1.17.2",
    "command-line-args": "^4.0.6",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.4.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "http-server": "^0.12.3",
    "jasmine": "^3.6.4",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.1",
    "moment": "^2.29.1",
    "nodemon": "^1.11.0",
    "password-validator": "^5.1.1",
    "rxjs": "^6.5.4",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.4",
